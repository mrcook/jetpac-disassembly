 $753f call $7309      ; Read Left/Right input
 $7542 bit 3,a         ; {Walk RIGHT
 $7544 jr z,$75ad      ; }
 $7546 bit 4,a         ; {Walk LEFT
 $7548 jp z,$75bf      ; }
 $754b ld (ix+$05),$00 ; else set Jetman X speed to zero
