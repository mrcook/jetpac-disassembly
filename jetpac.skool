 $73e0 add hl,hl      ;
 $73e1 add hl,hl      ; }
 $73e2 ld d,(ix+$01)  ; #REGd=Jetman X position
@ssub=ld a,($5dc0+$07) ; Actor thrust
 $73e5 ld a,($5dc7)   ;
 $73e8 ld e,a         ;
